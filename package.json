{
    "name": "update-changelog-deps-action",
    "version": "1.1.0",
    "description": "GitHub Action to update CHANGELOG.md with changed dependencies.",
    "author": {
        "name": "Felix Rizzolli",
        "email": "felix.rizzolli@outlook.de",
        "url": "https://github.com/FelixRizzolli"
    },
    "license": "MIT",
    "main": "dist/index.js",
    "scripts": {
        "build": "tsc && ncc build lib/index.js -o dist --source-map --license licenses.txt",
        "type-check": "tsc --noEmit",
        "format": "oxfmt .",
        "format:check": "oxfmt --check src",
        "lint": "oxlint src",
        "test": "vitest run",
        "test:watch": "vitest",
        "test:coverage": "vitest run --coverage"
    },
    "keywords": [],
    "packageManager": "pnpm@10.18.3",
    "dependencies": {
        "@actions/core": "^1.11.1",
        "@actions/exec": "^1.1.1",
        "@actions/github": "^6.0.1"
    },
    "devDependencies": {
        "@types/node": "^24.10.0",
        "@vercel/ncc": "^0.38.4",
        "@vitest/coverage-v8": "^4.0.7",
        "oxfmt": "^0.13.0",
        "oxlint": "^1.26.0",
        "typescript": "^5.9.3",
        "vitest": "^4.0.7"
    }
}
